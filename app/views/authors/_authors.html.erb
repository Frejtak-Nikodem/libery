<table style="border-collapse: collapse; ">
    <thead>
      <tr style="border-style: solid; border-width: 1px;">
        <th>Number |</th>
        <th>Last Name</th>
      </tr>
    </thead>

    <tbody>
        <% @q.each do |q| %>
            <tr style="border-style: solid; border-width: 1px;">
                <td><%= q.id %></td>
                <td><%= q.last_name %></td>
                <td><%= link_to '| Check', q %></td>
            </tr>
        <% end %>
    </tbody>
  </table>
